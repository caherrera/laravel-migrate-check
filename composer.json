{
    "name": "caherrera/laravel-migrate-check",
    "description": "A command to check for pending migrations. Exits with number of pending migrations if there are any",
    "keywords": [
        "migrate",
        "check",
        "eloquent",
        "database",
        "laravel-migrate-check"
    ],
    "homepage": "https://github.com/caherrera/laravel-migrate-check",
    "license": "MIT",
    "authors": [
        {
            "name": "Carlos Herrera",
            "email": "caherrera@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/console": "^5.5.17",
        "illuminate/events": "^5.5.17"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "caherrera\\MigrateCheck\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "caherrera\\MigrateCheck\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "/vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "caherrera\\MigrateCheck\\MigrateCheckServiceProvider"
            ]
        }
    }
}
